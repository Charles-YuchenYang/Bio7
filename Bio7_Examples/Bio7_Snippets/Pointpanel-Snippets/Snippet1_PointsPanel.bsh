/* This example shows how to get the points from the Points panel.*/

import java.awt.geom.Point2D;

Point2D.Double[]point=PointPanel.getPoints();
	

for (int i = 0; i < point.length; i++) {
	double x = point[i].getX();
	double y = point[i].getY();

	System.out.println(i+1+": "+"X: " + x + " " + "Y: " + y);

}